<Window x:Class="Viking_Rejser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Viking_Rejser"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Margin="10,0,9.6,0">
            <Button x:Name="mainPage" Content="Forside" MinHeight="30" MinWidth="50" Margin="0 0 10 0" Click="mainPage_Click"/>
            <Button x:Name="rejser" Content="rejsearrangement" Width="120" Click="rejser_Click" />
            <Button x:Name="Transportører" Content="Transportører" HorizontalAlignment="Left" Margin="20,0,10,0" VerticalAlignment="Top" Width="120" Height="28" Click="Transportører_Click"/>
        </StackPanel>
        <Frame x:Name="Main" NavigationUIVisibility="Hidden"/>
        

    </Grid>
</Window>
